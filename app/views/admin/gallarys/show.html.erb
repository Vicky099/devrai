<div class="news">
  <div class="container">
    <h3>Gallary</h3>
    <div class="col-md-6">
      <div class="agileits_news_grid_left_gridl">
        <% photo_date = (@gallary.photo_date || @gallary.created_at ) %>
        <p class="para1"><%= photo_date.day %><span><%= photo_date.strftime("%b-%Y")%></span></p>
      </div>
      <div class="agileits_news_grid_left_gridr">
        <%= cl_image_tag(@gallary.photo_url, :width => 100, :height => 150, :crop => :fill, class: 'thumbnail')%>
        <h4><%= link_to @gallary.name, "#" %></h4>
        <p><%= "Devrai's best image" %></p>
      </div>
    </div>
  </div>
</div>
