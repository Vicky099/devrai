<div class="banner-w3l">
  <div class="banner-text-w3ls" style="color:white;">
    <h3>Member Registration</h3>
      <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
        <div class="form-group">
          <%= f.label :full_name%>
          <%= f.text_field :full_name, class: 'form-control', required: true %>
        </div>
        <div class="form-group">
          <%= f.label :date_of_birth%>
          <%= f.date_field :date_of_birth, class: 'form-control', required: true %>
        </div>
        <div class="form-group">
          <%= f.label :contact_no%>
          <%= f.text_field :contact_no, class: 'form-control', required: true %>
        </div>
         <div class="form-group">
          <%= f.label :address %>
          <%= f.text_area :address, class: 'form-control', required: true %>
        </div>
        <div class="form-group">
          <%= f.label :state%>
          <%= f.text_field :state, class: 'form-control', required: true %>
        </div>
        <div class="form-group">
          <%= f.label :city %>
          <%= f.text_field :city, class: 'form-control', required: true %>
        </div>
        <div class="form-group">
          <%= f.label :email%><br />
          <%= f.email_field :email, autofocus: true, class: 'form-control', required: true %>
        </div>
        <div class="form-group">
          <%= f.label :password %>
          <% if @minimum_password_length %>
          <em>(<%= @minimum_password_length %> characters minimum)</em>
          <% end %><br />
          <%= f.password_field :password, autocomplete: "off", class: 'form-control', required: true %>
        </div>
        <div>
          <%= f.label :password_confirmation %><br />
          <%= f.password_field :password_confirmation, autocomplete: "off", class: 'form-control', required: true %>
        </div>
        <% if devise_mapping.rememberable? -%>
          <div class="form-group">
            <%= f.check_box :remember_me %>
            <%= f.label :remember_me%>
          </div>
        <% end -%>
        <%= f.submit "Sign up", class: "btn btn-default" %>
      <% end %>
      <%= render "devise/shared/links" %>
  </div>
</div>
