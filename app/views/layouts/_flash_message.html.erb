<%= content_tag 'p', (flash[:notice]).html_safe, class: 'notice aalert alert-success success-message' if flash[:notice] %>
<%= content_tag 'p', (flash[:error] || flash[:alert]).html_safe, class: 'error aalert alert-danger error-message' if flash[:error] || flash[:alert] %>
<% flash.clear if(request.path.split("/").last != "thank-you") %>
